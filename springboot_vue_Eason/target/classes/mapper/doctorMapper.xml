<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.eason.dao.DoctorMapper">
    <select id="loadDoctor" resultType="doctor" parameterType="doctor">
        select * from tb_doctor
        <where>
        tb_doctor_phone=#{tb_doctor_phone} and tb_doctor_password=#{tb_doctor_password}
        </where>
    </select>
    <select id="loadAll" resultType="doctor">
        select * from tb_doctor
    </select>
</mapper>