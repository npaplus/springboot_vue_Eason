<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.eason.dao.ClassRoomMapper">
    <resultMap id="room" type="com.qf.eason.pojo.ClassRoom">
        <id column="tb_classroom_id" property="roomId"></id>
        <result column="tb_classroom_name" property="roomName"></result>
        <result column="tb_classroom_text" property="roomText"></result>
    </resultMap>
    <select id="findAll" resultMap="room">
        select * from tb_classroom
    </select>
</mapper>